<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.2//EN" "http://gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module rename-to="GeneralPageReactJava">

   <!-- Inherit the ReactJava module                         -->
   <inherits name='ReactJava'/>

   <!-- transcode the application sources                    -->
   <source path="components/generalpage" />
   <source path="components/pdfviewer" />
   <source path="examples/generalpage" />

   <!-- Specify the app entry point class.                   -->
   <entry-point class='io.reactjava.client.examples.generalpage.App'/>

   <!-- Combine all platform-specific versions               -->
   <collapse-all-properties />
</module>
